<script src="sc.js" defer></script>
<style>
    #fk{
        width: 50%;
        position: relative;
        left: 25%;
        top: 25%;
    }
    #video-grid {
      display: grid;
      grid-template-columns: repeat(auto-fill, 300px);
      grid-auto-rows: 300px;
      background-color: black;
    }
    #s{
      width: 100%;
      background-color: black;
    }
    
    video {
      width: 100%;
      height: 100%;
      object-fit: cover;
      margin-left: 2px;
    }
    #nav{
      background-color: rgb(0, 110, 255);
      padding-top: 5px;
      padding-bottom: 5px;
      color:white;
      text-align: center;
    }
    #p{
      float:left;
      width:20%;
      height:100%;
      background-color: bisque;
    }
</style>
    <body>
        <div id='fk'>
    <form>
  <div class="mb-3">
    <img src="zoom.png" class="img-fluid"></img><br><br><br>
        <label class="form-label">Room</label>
    <input class="form-control"  aria-describedby="emailHelp" id="rm" required="false"> 
    <div id="emailHelp" class="form-text">Dont need to fill this up if you are creating a room</div>
    <label for="exampleInputEmail1" class="form-label">Username</label>
    <div class="input-group mb-3">
        <span class="input-group-text" id="basic-addon1">@</span>
        <input type="text" class="form-control" placeholder="Username" aria-label="Username" aria-describedby="basic-addon1" id="um" required="false">
      </div>
  </div>
  <button type="submit" class="btn btn-primary" id="j">Join Room</button>
  <button type="submit" class="btn btn-primary" id="c">Create Room</button>
</form>
        </div>
    </body>
</html>